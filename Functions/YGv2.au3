			$search = _ImageSearchArea("INI\" & $i & ".bmp",0,$Q11, $Q12, $Q13, $Q14,$x,$y,0)
			If $i = 401 Then ExitLoop
		WEnd
		$N = PixelChecksum($Q11, $Q12, $Q13, $Q14)
		IniWrite("INI\QTY.ini", "PIXELS", $N, $i)
		If $DEBUG = "YES" Then ToolTip("FIXED",0,0)
		FileClose("QTY.ini")
		_AmountYG()
	EndIf

EndIf

EndFunc

