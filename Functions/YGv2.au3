	$Q12 = $Q2 - 1
	$Q13 = $Q3 + 9
	$Q14 = $Q4 + 3
	$x = $Q11
	$y = $Q12
	$i = 0
	Local $search

	$N = PixelChecksum($Q11, $Q12, $Q13, $Q14)
	$YG = IniRead("INI\QTY.ini", "PIXELS", $N, 401)

	If $YG = 401 Then
		If $DEBUG = "YES" Then ToolTip("Auto-fixing Quantity",0,0)
